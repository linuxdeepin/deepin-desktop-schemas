<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
	<schema id="com.deepin.dde.keybinding.system" path="/com/deepin/dde/keybinding/system/">
		<key type="as" name="launcher">
			<default>['Super_L','Super_R']</default>
			<summary>Show or hide dde-launcher</summary>
			<description></description>
		</key>
		<key type="as" name="switch-monitors">
			<default><![CDATA[['<Super>P']]]></default>
			<summary>allow switch monitors display mode</summary>
			<description></description>
		</key>
		<key type="as" name="terminal">
			<default><![CDATA[['<Control><Alt>T']]]></default>
			<summary>Launch default terminal emulator</summary>
			<description></description>
		</key>
		<key type="as" name="deepin-screen-recorder">
			<default><![CDATA[['<Control><Alt>R']]]></default>
			<summary>Launch screen recorder</summary>
			<description></description>
		</key>
		<key type="as" name="system-monitor">
			<default><![CDATA[['<Control><Alt>Escape']]]></default>
			<summary>Launch system monitor</summary>
			<description></description>
		</key>
		<key type="as" name="lock-screen">
			<default><![CDATA[['<Super>L']]]></default>
			<summary>Lock screen</summary>
			<description></description>
		</key>
		<key type="as" name="logout">
			<default><![CDATA[['<Control><Alt>Delete','<Control><Alt>KP_Delete','<Control><Alt>.']]]></default>
			<summary>Show shutdown interface</summary>
			<description></description>
		</key>
		<key type="as" name="terminal-quake">
			<default><![CDATA[['<Alt>F2']]]></default>
			<summary>Show or hide quake mode deepin terminal</summary>
			<description></description>
		</key>
		<key type="as" name="screenshot">
			<default><![CDATA[['<Control><Alt>A']]]></default>
			<summary>Take a screenshot</summary>
			<description></description>
		</key>
		<key type="as" name="screenshot-fullscreen">
			<default>['Print']</default>
			<summary>Take a screenshot the whole screen</summary>
			<description></description>
		</key>
		<key type="as" name="screenshot-window">
			<default><![CDATA[['<Alt>Print']]]></default>
			<summary>Take a screenshot of the most top window</summary>
			<description></description>
		</key>
		<key type="as" name="screenshot-delayed">
			<default><![CDATA[['<Control>Print']]]></default>
			<summary>Take a screenshot after a few seconds</summary>
			<description></description>
		</key>
		<key type="as" name="file-manager">
			<default><![CDATA[['<Super>E']]]></default>
			<summary>Launch default file manager</summary>
			<description></description>
		</key>
		<key type="as" name="disable-touchpad">
			<default>['']</default>
			<summary>Disable touchpad</summary>
			<description></description>
		</key>
		<key type="as" name="wm-switcher">
			<default><![CDATA[['<Shift><Super>Tab']]]></default>
			<summary>Switch window effects</summary>
			<description></description>
		</key>
		<key type="as" name="turn-off-screen">
			<default><![CDATA[['<Shift><Super>L']]]></default>
			<summary>Turn off the screen</summary>
			<description/>
		</key>
		<key type="as" name="ai-assistant">
			<default><![CDATA[['<Super>Q']]]></default>
			<summary>Desktop AI Assistant</summary>
			<description/>
		</key>
		<key type="as" name="text-to-speech">
			<default><![CDATA[['<Control><Alt>P']]]></default>
			<summary>Text to speech</summary>
			<description/>
		</key>
		<key type="as" name="speech-to-text">
			<default><![CDATA[['<Control><Alt>D']]]></default>
			<summary>Speech to text</summary>
			<description/>
		</key>
		<key type="as" name="clipboard">
			<default><![CDATA[['<Super>V','<Meta>V']]]></default>
			<summary>Show clipboard</summary>
			<description/>
		</key>
		<key type="as" name="translation">
			<default><![CDATA[['<Control><Alt>U']]]></default>
			<summary>Translation</summary>
			<description/>
		</key>
		<key type="as" name="notification-center">
			<default><![CDATA[['<Super>M','<Meta>M']]]></default>
			<summary>open notification center</summary>
			<description/>
		</key>
		<key type="as" name="screenshot-ocr">
			<default><![CDATA[['<Control><Alt>C']]]></default>
			<summary>Take a screenshot for ocr</summary>
			<description></description>
		</key>
		<key type="as" name="screenshot-scroll">
			<default><![CDATA[['<Control><Alt>I']]]></default>
			<summary>Take a screenshot the scroll screen</summary>
		</key>
		<key type="as" name="global-search">
			<default><![CDATA[['<Shift>space']]]></default>
			<summary>filemanager global search</summary>
			<description></description>
		</key>
	</schema>

	<schema id="com.deepin.dde.keybinding.system.platform" path="/com/deepin/dde/keybinding/system/platform/">
		<key type='as' name="global-search">
			<default>['All']</default>
			<summary>filemanager global search</summary>
			<description></description>
		</key>
	</schema>

	<schema id="com.deepin.dde.keybinding.system.enable" path="/com/deepin/dde/keybinding/system/enable/">
		<key type="b" name="launcher">
			<default>true</default>
			<summary>enabled/disable launcher</summary>
			<description></description>
		</key>
		<key type="b" name="screenshot-delayed">
			<default>true</default>
			<summary>enabled/disable screenshot-delayed</summary>
			<description></description>
		</key>
		<key type="b" name="screenshot-fullscreen">
			<default>true</default>
			<summary>enabled/disable screenshot-fullscreen</summary>
			<description></description>
		</key>
		<key type="b" name="deepin-screen-recorder">
			<default>true</default>
			<summary>enabled/disable deepin-screen-recorder</summary>
			<description></description>
		</key>
		<key type="b" name="translation">
			<default>true</default>
			<summary>enabled/disable translation</summary>
			<description></description>
		</key>
		<key type="b" name="screenshot">
			<default>true</default>
			<summary>enabled/disable screenshot</summary>
			<description></description>
		</key>
		<key type="b" name="logout">
			<default>true</default>
			<summary>enabled/disable logout</summary>
			<description></description>
		</key>
		<key type="b" name="lock-screen">
			<default>true</default>
			<summary>enabled/disable lock-screen</summary>
			<description></description>
		</key>
		<key type="b" name="clipboard">
			<default>true</default>
			<summary>enabled/disable clipboard</summary>
			<description></description>
		</key>
		<key type="b" name="notification-center">
			<default>true</default>
			<summary>enabled/disable notification-center</summary>
			<description></description>
		</key>
		<key type="b" name="disable-touchpad">
			<default>true</default>
			<summary>enabled/disable disable-touchpad</summary>
			<description></description>
		</key>
		<key type="b" name="screenshot-window">
			<default>true</default>
			<summary>enabled/disable screenshot-window</summary>
			<description></description>
		</key>
		<key type="b" name="turn-off-screen">
			<default>true</default>
			<summary>enabled/disable turn-off-screen</summary>
			<description></description>
		</key>
		<key type="b" name="terminal">
			<default>true</default>
			<summary>enabled/disable terminal</summary>
			<description></description>
		</key>
		<key type="b" name="file-manager">
			<default>true</default>
			<summary>enabled/disable file-manager</summary>
			<description></description>
		</key>
		<key type="b" name="system-monitor">
			<default>true</default>
			<summary>enabled/disable system-monitor</summary>
			<description></description>
		</key>
		<key type="b" name="global-search">
			<default>true</default>
			<summary>enabled/disable global-search</summary>
			<description></description>
		</key>
		<key type="b" name="text-to-speech">
			<default>true</default>
			<summary>enabled/disable text-to-speech</summary>
			<description></description>
		</key>
		<key type="b" name="speech-to-text">
			<default>true</default>
			<summary>enabled/disable speech-to-text</summary>
			<description></description>
		</key>
		<key type="b" name="terminal-quake">
			<default>true</default>
			<summary>enabled/disable terminal-quake</summary>
			<description></description>
		</key>
		<key type="b" name="ai-assistant">
			<default>true</default>
			<summary>enabled/disable ai-assistant</summary>
			<description></description>
		</key>
		<key type="b" name="wm-switcher">
			<default>true</default>
			<summary>enabled/disable wm-switcher</summary>
			<description></description>
		</key>
	</schema>
</schemalist>
